{
  "name": "trood-restify",
  "description": "Extension for RESTfull api communications and data management",
  "version": "0.0.1",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "lodash": "^4.17.20",
    "mobx-state-tree": "^3.17.2",
    "object-hash": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "pre-commit": "^1.2.2",
    "typescript": "^4.0.2",
    "ts-node": "^10.9.1",
    "esbuild": "^0.14.49"
  },
  "peerDependencies": {
    "mobx": "^5.15.6"
  },
  "scripts": {
    "precommit": "npm run lint",
    "lint": "eslint src/ --ext .js",
    "ts:defs": "tsc --declaration --emitDeclarationOnly -p tsconfig.json",
    "build": "npm run ts:defs && cross-env NODE_ENV=production && node ./esbuild-config.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Q-CIS-DEV/trood-restify.git"
  }
}
